#yolo
Order deny,allow
Allow from all

RedirectMatch 403 ^.*/\.f/.*

#as is
RewriteEngine On

#directory
DirectoryIndex index.html

#www to non-www
RewriteBase /
RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
RewriteRule ^(.*)$ http://%1/$1 [R=301,L]

#things
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule (.*) dl.php?$1 [NC,L]
